 lang="ja"
 title="接続"
 autoWireViewModel="true">
    <local:AccountsPage.head>
        <HeadItems>
            <Style uri="{resource Styles}"/>
            <Style uri="{resource Themes}"/>
            <Script uri="st-resource://jquery.js"/>
